test : grades sectionA.txt
	./grades <sectionA.txt
	
grades : grades.c
	gcc -g -Wall -o grades grades.c
	
clean :
	-rm grades
	
submit: loop.c
	mkdir lab02_$(USER)
	cp loop.c buggy.c lab02_$(USER)
	tar -cvzf lab02_$(USER).tar.gz lab02_$(USER)
	rm -r lab02_$(USER)
	echo Turn in lab02_$(USER).tar.gz on myCourses	